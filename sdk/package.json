  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "plot": "./bin/plot.cjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./agent": {
      "types": "./dist/agent.d.ts",
      "default": "./dist/agent.js"
    },
    "./tool": {
      "types": "./dist/tool.d.ts",
      "default": "./dist/tool.js"
    },
